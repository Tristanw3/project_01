<%= form_for @course do |c| %>

  <%= c.label 'Course Name' %><br>
  <%= c.text_field :course_name %><br>

  <%= c.label 'Image' %><br>
  <%= c.text_field :image %><br>

  <%= c.label 'Link' %><br>
  <%= c.text_field :link %><br>

  <%= c.label 'Description' %><br>
  <%= c.text_field :description %><br>

  <%= c.submit "Send Edit" %>
  
<% end %>
