<%= form_for @user do |c| %>

  <%= c.label 'User Name' %><br>
  <%= c.text_field :user_name %><br>
  
  <%= c.label 'Name' %><br>
  <%= c.text_field :name %><br>

  <%= c.label 'Link' %><br>
  <%= c.text_field :link %><br>

  <%= c.label 'Description' %><br>
  <%= c.text_field :description %><br>

  <%= c.submit "Send Edit" %>
  
<% end %>
